{
  "version": "2.0",
  "functionTimeout": "00:00:30",
  "extensions": {
    "durableTask": {
      "storageProvider": {
        "partitionCount": "1",
        "workItemQueueVisibilityTimeout": "00:00:15"
      },
      "maxConcurrentActivityFunctions": "1",
      "maxConcurrentOrchestratorFunctions": "1"
    }
  },
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            },
            "enableLiveMetricsFilters": true
        }
    }
}